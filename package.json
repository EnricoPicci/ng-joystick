{
  "name": "ng-joystick-app",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-library": "if [ -d 'dist' ]; then rm -Rf dist; fi && ng build ng-joystick --prod",
    "build-app": "ng build ng-joystick-app --prod",
    "build-all": "npm run build-library && npm run build-app",
    "npm_publish": "cd dist/ng-joystick && npm publish",
    "publish": "npm run build-library && cd dist/ng-joystick && npm publish",
    "test-lib-headless": "ng test ng-joystick --browsers ChromeHeadlessNoSandbox --watch=false",
    "test-app-headless": "ng test ng-joystick-app --browsers ChromeHeadlessNoSandbox --watch=false ",
    "test-app-headless-coverage": "npm run test-app-headless-coverage --code-coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/animations": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "coveralls": "^3.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "^5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnricoPicci/ng-joystick.git"
  },
  "keywords": [
    "joystick",
    "observable",
    "rxjs",
    "angular"
  ],
  "author": "Enrico",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EnricoPicci/ng-joystick/issues"
  },
  "homepage": "https://github.com/EnricoPicci/ng-joystick#readme"
}
